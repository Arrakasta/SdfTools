    </Reference>
    <Reference Include="OSGeo.FDO.Common">
      <HintPath>bin\Debug\net8.0-windows\FDO\OSGeo.FDO.Common.dll</HintPath>
    </Reference>
    <Reference Include="OSGeo.FDO.Geometry">
      <HintPath>bin\Debug\net8.0-windows\FDO\OSGeo.FDO.Geometry.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
